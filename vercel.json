{
      "version": 2,
      "builds": [
        {
          "src": "src/utils/app.js",
          "use": "@vercel/node",
          "config":{"includeFiles": ["dist/**"]}
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "src/utils/app.js" 
        }
      ]
    }